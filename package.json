{
  "name": "photo-website",
  "version": "0.1",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/styles": "^4.11.3",
    "@testing-library/user-event": "^12.8.3",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-router-transition": "^2.1.0",
    "react-scripts": "4.0.3",
    "react-transition-group": "^4.4.1",
    "typescript": "^4.2.3",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cypress": "wait-on http://localhost:3000 && CI=true npx cypress run --headless",
    "integration": "concurrently --kill-others \"npm run start\" \"npm run cypress\"",
    "format": "npx prettier --config .prettierrc.js \"src/**/*.(ts(x)?)\" --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.22",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-router-transition": "^2.1.0",
    "@types/react-test-renderer": "^17.0.1",
    "concurrently": "^6.0.0",
    "cypress": "^6.8.0",
    "jest": "^26.6.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.2",
    "wait-on": "^5.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*+(.tsx|.ts)",
      "!src/images/**/*.ts",
      "!**/__test__/",
      "!**/serviceWorker.ts",
      "!**/react-app-env.d.ts",
      "!**/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": -16
      }
    }
  }
}
