{
  "name": "photo-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/styles": "^4.11.3",
    "@testing-library/user-event": "^12.8.3",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/uuid": "^8.3.0",
    "emailjs-com": "^2.6.4",
    "normalize-wheel": "^1.0.1",
    "open-sans-fonts": "^1.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-router-transition": "^2.1.0",
    "react-scripts": "4.0.3",
    "react-transition-group": "^4.4.1",
    "typescript": "^4.2.3",
    "underscore": "^1.12.1",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:nowatch": "react-scripts start --watchAll=false",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "eject": "react-scripts eject",
    "format": "npx prettier --config .prettierrc.js \"src/**/*.(ts(x)?)\" --write",
    "cypress": "wait-on http://localhost:3000 && CI=true npx cypress run --headless",
    "integration": "concurrently --kill-others \"npm run start:nowatch\" \"npm run cypress\"",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.22",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-router-transition": "^2.1.0",
    "@types/react-test-renderer": "^17.0.1",
    "@types/underscore": "^1.11.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "concurrently": "^6.0.0",
    "cypress": "^6.8.0",
    "eslint": "^7.23.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.1",
    "husky": "^6.0.0",
    "jest": "^26.6.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.2",
    "wait-on": "^5.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*+(.tsx|.ts)",
      "!src/images/**/*.ts",
      "!**/__test__/",
      "!**/serviceWorker.ts",
      "!**/react-app-env.d.ts",
      "!**/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": -16
      }
    }
  }
}
